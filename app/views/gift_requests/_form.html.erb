<%= simple_form_for @giftrequest do |f| %>
  <%= f.input :recipient_name %>
  <%= f.input :recipient_address %>
  <%= f.input :delivery_due_date %>
  <%= f.input :budget %>
  <%= f.input :packaging %>
  <%= f.input :comment %>
  <%= f.input :product1 %>
  <%= f.input :shop1 %>
  <%= f.input :product2 %>
  <%= f.input :shop2 %>
  <%= f.input :product3 %>
  <%= f.input :shop3 %>
  <%= f.submit %>
<% end %>
